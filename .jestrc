{
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>[/\\\\](build|node_modules)[/\\\\]"
  ],
  "testURL": "http://localhost",
  "moduleNameMapper": {
    "^.+\\.s?css$": "identity-obj-proxy",
    "^.+\\.(svg|woff2?|ttf|eot|jpe?g|png|gif)$": "null-loader"
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
  ],
  "coverageReporters": ["text-summary", "html"]
}
